{
    "name": "jonfackrell/ebsco-discovery-api-sdk",
    "description": "EBSCO Discovery API SDK Library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Fackrell",
            "email": "jon.fackrell@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JonFackrell\\Eds\\": "src/"
        }
    },
    "autoload-dev": {
